/* pages/me/index.wxss */
@charset "UTF-8";

page{
    background:rgba(245,245,245,1);
}
.ui-main {
    width: 100%;
    padding-bottom: 118rpx;
    background: #ccc
}

.ui-wrap {
    position: relative;
    width: 100%;
    height: 312rpx;
    padding: 30rpx 30rpx 0;
    /*background-color: #FFF5DC;*/
    background-size: 100% 100%;
    background-repeat: no-repeat;
}

.ui-myInfo {
    display: flex;
    align-items: center;
}

.ui-avatar {
    width: 120rpx;
}

.ui-avatar__btn {
    width: 120rpx;
    height: 120rpx;
    /*background-color: #f1f1f1;*/
    border-radius: 50%;
}

.ui-avatar__img {
    width: 120rpx;
    height: 120rpx;
    background-color: #f1f1f1;
    border-radius: 50%;
}

.ui-role {
    margin: 0 auto;
    width:112rpx;
    height:40rpx;
    line-height: 40 rpx;
    transform: translateY(-30rpx);
    background:linear-gradient(160deg,rgba(255,207,57,1) 0%,rgba(255,159,34,1) 100%);
    border-radius:20rpx;
    text-align: center;
    font-size: 24rpx;
    color: #fff;
}

.ui-profile {
    width: 200rpx;
    margin-left: 50rpx;
}
.ui-name {
    font-size:34rpx;
    font-weight:500;
    color:rgba(51,51,51,1);
    line-height:34rpx;
}

.ui-profile__id {
    margin-top: 25rpx;
    font-size:24rpx;
    color:rgba(51,51,51,1);
    line-height:24rpx;
}

.ui-sign {
    font-size:32rpx;
    color:rgba(51,51,51,1);
    line-height:100rpx;
    height: 100rpx;
}

.ui-operate {
    display: flex;
    justify-content: space-around;
    margin-left: auto;
}

.ui-operate__item {
    width: 100rpx;
    height: 100%;
    margin-left: 25rpx;
    text-align: center;
    background-color: transparent;
}
.ui-operate__icon {
    width: 44rpx;
    height: 44rpx;
}

.ui-icon__name {
    margin-top: 10rpx;
    font-size:20rpx;
    color:rgba(51,51,51,1);
    line-height:20rpx;
}

.ui-card {
    width: 670rpx;
    height: 100rpx;
    position: absolute;
    bottom: 0;
    left: 50%;
    transform: translateX(-50%);
}
.ui-card__bg {
    width: 670rpx;
    height: 100rpx;
}

.ui-member__img {
    position: absolute;
    width:32rpx;
    height: 32rpx;
    top: 50%;
    transform: translateY(-50%);
    left: 32rpx;
}

.ui-card__info {
    position: absolute;
    top: 50%;
    transform: translateY(-50%);
    left: 90rpx;
}

.ui-card__title {
    font-size:28rpx;
    color:rgba(251,225,159,1);
    line-height:28rpx;
}

.ui-card__desc {
    margin-top: 15rpx;
    font-size:20rpx;
    color:rgba(255,255,255,1);
    line-height:20rpx;
}

.ui-card__status {
    position: absolute;
    top: 50%;
    transform: translateY(-50%);
    right: 32rpx;
}

.icon-next {
    width: 28rpx;
    height: 28rpx;
}

.ui-card__none {
    position: relative;
    width:132rpx;
    height:48rpx;
    background:linear-gradient(90deg,rgba(244,215,145,1) 0%,rgba(251,225,160,1) 100%);
    border-radius:26rpx;
    font-size:24rpx;
    color:rgba(51,51,51,1);
    line-height:48rpx;
    text-align: center;
}

.ui-card__none:before{
    position: absolute;
    top: 50%;
    transform: translateY(-50%);
    right: 15rpx;
    content: "";
    width: 0;
    height: 0;
    border-top: 7rpx solid transparent;
    border-bottom: 7rpx solid transparent;
    border-left: 7rpx solid #000;
}

.ui-panel {
    display: flex;
    justify-content: space-around;
    align-items: center;
    margin: 30rpx auto;
    margin-bottom: 20rpx;
    width:702rpx;
    height: 160rpx;
    background:rgba(255,255,255,1);
    border-radius:16rpx;
}

.ui-panel__item {
    /* width: 75rpx;
    height: 80rpx; */
    text-align: center;
}
.ui-panel__num{
    font-size:36rpx;
    color:rgba(51,51,51,1);
    line-height:36rpx;
}

.ui-panel__name {
    margin-top: 15rpx;
    font-size:24rpx;
    color:rgba(102,102,102,1);
    line-height:24rpx;
}

.ui-myOrder{
    display: flex;
    flex-direction: column;
    width:702rpx;
    background:rgba(255,255,255,1);
    box-shadow:0rpx 4rpx 16rpx 0rpx rgba(0,0,0,0.05);
    border-radius:16rpx;
    margin: auto;
    padding-bottom: 40rpx;

}

.ui-orderAll{
    display: flex;
    justify-content: space-between;
    margin-top: 32rpx;
    width: 100%;
}

.ui-orderText{
    margin-left: 36rpx;
    height:34rpx;
    font-size:34rpx;
    font-family:PingFangSC;
    font-weight:400;
    color:rgba(51,51,51,1);
    line-height:34rpx;
}

.ui-orderCheck{
    display: flex;
    margin-top: 8rpx;
    margin-right: 36rpx;
    height: 26rpx;
}

.ui-orderCheck:active{
    opacity: 0.5;
}

.ui-orderCheck text{
    height:26rpx;
    font-size:26rpx;
    font-family:PingFangSC;
    font-weight:400;
    color:rgba(153,153,153,1);
    line-height:26rpx;
}

.ui-checkMoreOrder{
    margin-left: 8rpx;
    width: 28rpx;
    height: 28rpx;
}

.ui-orderStatus{
    display: flex;
}

.ui-feedbackItem{
    display: flex;
    justify-content: center;
    flex-direction: column;
}

.ui-feedbackItem:active{
    opacity: 0.5;
}
.ui-orderStatuaItem {
    position: relative;
    width: 100%;
    height: 100%;
    flex: 1;
}
.ui-iconFeedback{
    margin-top: 30rpx;
    width:48rpx;
    height:48rpx;
    margin-left: auto;
    margin-right: 30rpx;

}
.ui-feedbackItem .ui-orderText{
    display: block;
    height:22rpx;
    font-size:22rpx;
    font-family:PingFangSC-Medium;
    font-weight:500;
    color:rgba(102,102,102,1);
    line-height:22rpx;
    text-align: center;
    margin-top:20rpx;
}

.ui-notification{
    position: absolute;
    z-index: 0;
    margin: 12rpx 25rpx 0 0;
    display: flex;
    justify-content: center;
    align-items: center;
    top: 0;
    right: 0;
    transform: translateX(50%);
    height:35rpx;
    width: 35rpx;
    background:rgba(255,255,255,1);
    border:2rpx solid rgba(251, 88, 77, 1);
    border-radius:50%;
    box-sizing: border-box;
}

.ui-notification text{
    font-size:24rpx;
    font-family:PingFang-SC;
    /*font-weight:800;*/
    color:rgba(251, 88, 77, 1);
}

.ui-myServe {
    margin: 20rpx auto 0;
    padding: 30rpx;
    width:702rpx;
    /*height:390rpx;*/
    background:rgba(255,255,255,1);
    border-radius:16rpx;
}

.ui-myServe__title {
    font-size:32rpx;
    color:rgba(51,51,51,1);
    line-height:32rpx;
}

.ui-serve {
    width: 100%;
    /*margin-top: 30rpx;*/
    flex-wrap: wrap;
    display: flex;
    justify-content: flex-start;
}

.ui-serve__item {
    width: 126rpx;
    margin-top: 50rpx;
    margin-right: 47rpx;
    margin-left: 0rpx;
    text-align: center;
}
.ui-serve__item:nth-child(4n) {
    margin-right: 0;
}
.ui-serve__icon {
    width: 44rpx;
    height: 44rpx;
}

.ui-serve__name {
    margin-top: 10rpx;
    font-size:24rpx;
    color:rgba(102,102,102,1);
    line-height:24rpx;
}

/*mask*/
.ui-mask {
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    background-color: rgba(76, 76, 76, .7);
}
.ui-mask__closeIcon {
    position: absolute;
    top: 410rpx;
    right: 40rpx;
    width: 40rpx;
    height: 40rpx;
    z-index: 8;
}
.ui-mask__closeIcon image {
    width: 40rpx;
    height: 40rpx;
}
.ui-exchange {
    position: absolute;
    top: 400rpx;
    left: 50%;
    transform: translateX(-50%);
    width:694rpx;
    /*height:364rpx;*/
    background:#fff;
    border-radius:16rpx;
    z-index: 6;
}
.ui-modal__title {
    margin-top: 40rpx;
    text-align: center;
    font-size:32rpx;
    font-weight:500;
    color:rgba(61,61,61,1);
    line-height:32rpx;
}
.ui-input__cell {
    display: flex;
    align-items: center;
    width:630rpx;
    height:96rpx;
    padding: 24rpx;
    margin: 40rpx auto 50rpx;
    border-radius:8rpx;
    border:2rpx solid rgba(204,204,204,1);
}

.ui-cell__tips {
    width: 116rpx;
    height: 24rpx;
    font-size:24rpx;
    color:rgba(61,61,61,1);
    line-height:24rpx;
}

.ui-input {
    height: 24rpx;
    font-size: 24rpx;
    padding-left: 20rpx;
    color: #3D3D3D;
    line-height: 24rpx;
}

.placeholder-class {
    font-size: 24rpx;
    color: #ccc;
}

.ui-bottom {
    width: 100%;
    display: flex;
    align-items: center;
    border-top: 1rpx solid #eee;
}
.ui-cancel {
    width: 50%;
    height:93rpx;
    font-size:32rpx;
    font-weight:600;
    color:rgba(51,51,51,1);
    line-height:93rpx;
    text-align: center;
    border-right: 1rpx solid #eee;
}

.ui-ok {
    width: 50%;
    height:93rpx;
    font-size:32rpx;
    font-weight:600;
    color:rgba(89,118,178,1);
    line-height:93rpx;
    text-align: center;
}

.ui-inviter__wrap {
    position: absolute;
    top: 400rpx;
    left: 50%;
    transform: translateX(-50%);
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 25rpx;
    width:694rpx;
    background:#fff;
    border-radius:16rpx;
    z-index: 6;
}

.ui-inviter__info {
    display: flex;
    align-items: center;
}

.ui-inviter__img {
    width: 120rpx;
    height: 120rpx;
    border-radius: 50%;
    background-color: #f1f1f1;

}

.ui-inviter__name {
    margin-left: 20rpx;
    font-size:34rpx;
    color:rgba(51,51,51,1);
    line-height:34rpx;
}

.ui-mask__close image {
    width: 40rpx;
    height: 40rpx;
}
.ui-overOrderBox {
	display: flex;
	width: 614rpx;
	height: 84rpx;
	margin: 0 auto;
	margin-top: 24rpx;
	background:rgba(255,240,241,1);
    border-radius:12rpx;
	align-items: center;
	justify-content: center;

}
.ui-overOrderItem__img {
	width: 52rpx;
	height: 52rpx;
	margin-left: 24rpx;
}
.ui-overOrderItem__text {
	font-size:24rpx;
    font-family:PingFangSC-Regular,PingFangSC;
    font-weight:400;
    color:rgba(246,151,158,1);
	line-height:24rpx;
	margin-left: 24rpx;
}
.ui-overOrderItem__btn {
	display: flex;
	width:116rpx;
	height:48rpx;
	margin-left: auto;
	margin-right: 24rpx;
	border-radius:24px;
	border:1px solid rgba(255,197,201,1);
	justify-content: center;
	align-items: center;
}

.ui-myTool {
    margin: 20rpx auto 0;
    padding: 30rpx;
    width:702rpx;
    /*height:390rpx;*/
    background:rgba(255,255,255,1);
    border-radius:16rpx;
}

.ui-myTool__title {
    font-size:32rpx;
    color:rgba(51,51,51,1);
    line-height:32rpx;
}

.ui-tool {
    width: 100%;
    /*margin-top: 30rpx;*/
    flex-wrap: wrap;
    display: flex;
    justify-content: flex-start;
}

.ui-tool__item {
    width: 122rpx;
    margin-top: 50rpx;
    margin-right: 8rpx;
    margin-left: 0rpx;
    text-align: center;
}
.ui-tool__item:nth-child(5n) {
    margin-right: 0;
}
.ui-tool__icon {
    width: 50rpx;
    height: 50rpx;
}

.ui-tool__name {
    margin-top: 10rpx;
    font-size:24rpx;
    color:rgba(102,102,102,1);
    line-height:24rpx;
}
