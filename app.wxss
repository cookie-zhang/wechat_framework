.ui-richImg {
    width: 750rpx;
    height: auto;
}

form {}

.ui-submitBtn {
    padding: 0;
    margin: 0;
    width: 100%;
    height: 100%;
    background-color: #fff;
    border-style: none;
}

.button-hover {
    background-color: #fff;
    border-style: none;
}

button {
    line-height: 1;
    font-size: 0;
    padding: 0;
    border-radius: 0;
    background-color: #fff;
    outline: none;
    border: none;
}

button::hover {
    border-style: none;
    border: none;
    content: none;
    outline: none;
}

button:after {
    border-style: none;
    border: none;
    content: none;
    outline: none;
}

button::after {
    border-style: none;
    border: none;
    content: none;
    outline: none;
}

input-placeholder {
  color: #C1C1C1;
}

@font-face {
    font-family: 'iconfont';
    src: url('data:application/font-woff2;charset=utf-8;base64,') format('woff2'),
        url('data:application/font-woff;charset=utf-8;base64,') format('woff');
    font-weight: normal;
    font-style: normal;
    font-display: swap;
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-location:before {
  content: "\e61f";
}

