/**app.wxss**/
@import "libs/weui.wxss";
/**colorui**/
@import "colorui/main.wxss";
@import "colorui/icon.wxss";


page {
    __mainColor: #FFBE42;
    __mainFontColor: #333333;
    __disableColor: #ffffff;
    __bgColor: #ffffff;
    __lineColor: #e5e5e5;

    background-color: var(__bgColor);
    box-sizing: border-box;
}

swiper {
    width: 100%;
}

swiper image {
    width: 100%;
}

/*.ui-main {
    box-sizing: border-box;
}*/

page {
    box-sizing: border-box;
    background-color: #F0F1F3;
}

view,
image,
swiper,
icon,
text,
scroll-view,
cover-view,
cover-image {
    box-sizing: border-box;
}

